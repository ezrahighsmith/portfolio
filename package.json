{
  "name": "portfoliov2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.2",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-transition-group": "^4.2.3",
    "mime-types": "^2.1.25",
    "npm": "^6.13.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-transition-group": "^4.3.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start && npm run css-watch",
    "build": "react-scripts build && node-sass --omit-source-map-url ./src/sass/main.scss ./src/css/main.css",
    "deploy": "npm run build&&gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css-watch": "react-scripts build && node-sass --omit-source-map-url ./src/sass/main.scss ./src/css/main.css --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bulma": "^0.8.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.12.0"
  }
}